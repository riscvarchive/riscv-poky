+      ;;
+  esac
+  c_target_objs="${c_target_objs} glibc-c.o"
+  cxx_target_objs="${cxx_target_objs} glibc-c.o"
+  tmake_file="${tmake_file} t-glibc"
+  target_has_targetcm=yes
+  ;;
+*-*-netbsd*)
+  tmake_file="t-slibgcc"
+  gas=yes
+  gnu_ld=yes
